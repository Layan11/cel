<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Bloom?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="270.0" prefWidth="461.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.buy_ticketController">
   <children>
      <ImageView fitHeight="272.0" fitWidth="467.0">
         <image>
            <Image url="@newPics/seatscinema.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btn_choose" layoutX="338.0" layoutY="236.0" mnemonicParsing="false" onAction="#choose_seat" style="-fx-background-color: black;" text="Show Seats Map" textFill="WHITE">
         <font>
            <Font name="System Bold" size="13.0" />
         </font></Button>
      <Button fx:id="back" layoutX="14.0" layoutY="239.0" mnemonicParsing="false" onAction="#goBack" style="-fx-background-color: black;" text="Back" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <effect>
            <Bloom />
         </effect>
      </Button>
   </children>
</AnchorPane>
